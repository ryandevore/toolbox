#import <Availability.h>


#ifndef __IPHONE_6_0
#warning "This project uses features only available in iOS SDK 6.0 and later."
#endif

#ifdef __OBJC__
    #import <UIKit/UIKit.h>
    #import <Foundation/Foundation.h>
#endif

#define __UU_FILE__ [[NSString stringWithUTF8String:__FILE__] lastPathComponent]

// Custom UUDebugLog
#ifndef UUDebugLog
#ifdef DEBUG
#define UUDebugLog(fmt, ...) NSLog((@"%s [%@:%d] - " fmt), __PRETTY_FUNCTION__, __UU_FILE__, __LINE__, ##__VA_ARGS__);
#else
#define UUDebugLog( s, ... )
#endif // DEBUG
#endif // UUDebugLog

#define TRACE 1

#ifndef UUTrace
#ifdef TRACE
#define UUTrace() NSLog(@"TRACE %@:%d (%s)", __UU_FILE__, __LINE__, __PRETTY_FUNCTION__)
#else
#define UUTrace()
#endif // TRACE
#endif // UUTrace


// Switch UUDebugLog to use a SQLite based logger
#import "UUSqliteLog.h"

#undef UUDebugLog
#define UUDebugLog(fmt, ...) \
    [[UUSqliteLog sharedInstance] log:[NSString stringWithFormat:@"%s [%@:%d] - " fmt, __PRETTY_FUNCTION__, __UU_FILE__, __LINE__, ##__VA_ARGS__]]; \
    NSLog((@"%s [%@:%d] - " fmt), __PRETTY_FUNCTION__, __UU_FILE__, __LINE__, ##__VA_ARGS__);

#undef UUTrace
#define UUTrace() \
    [[UUSqliteLog sharedInstance] log:[NSString stringWithFormat:@"TRACE - %@:%d (%s)", __UU_FILE__, __LINE__, __PRETTY_FUNCTION__]]; \
    NSLog(@"TRACE %@:%d (%s)", __UU_FILE__, __LINE__, __PRETTY_FUNCTION__);


